<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.denny.automanifest" >

    <application
        android:name="com.denny.automanifests.AutoApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.denny.automanifests.AutoActivity"
            android:configChanges="fontScale"
            android:launchMode="singleTop"
            android:taskAffinity="com.denny.launcher" />
        <activity android:name="com.denny.automanifests.AutoAppActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>